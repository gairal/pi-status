doctype html
html(xmlns='http://www.w3.org/1999/xhtml', lang='en')
  head
    meta(http-equiv='Content-Type' content='text/html; charset=utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    title PI Status
    meta(name='description', content='')
    meta(name='robots', content='noindex, nofollow')
    meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no')
    link(rel='icon', type='image/webp', href='data:image/webp;base64,')

    meta(name='apple-mobile-web-app-capable', content='yes')
    meta(name='apple-mobile-web-app-status-bar-style', content='black')
    link(href='https://unpkg.com/tailwindcss@^1.0/dist/tailwind.min.css', rel='stylesheet')
  body.bg-black.text-gray-100
    main.flex.flex-col.p-16
      h1.text-center.text-6xl PI Status
      each p in pm2
        .w-full.rounded.overflow-hidden.py-4.px-6.mb-2(class='bg-'+p.color+'-500')
          .flex.justify-between.items-center
            h2.font-bold.text-xl.mb-2.text-gray-200 #{p.name}
            span.font-bold(class='text-'+p.color+'-900') #{p.status}
          span.inline-block.bg-gray-200.rounded-full.px-3.py-1.text-sm.font-semibold.text-gray-700.mr-2.mb-2 cpu: #{p.cpu}%
          span.inline-block.bg-gray-200.rounded-full.px-3.py-1.text-sm.font-semibold.text-gray-700.mr-2.mb-2 mem: #{p.memory}MB
          span.inline-block.bg-gray-200.rounded-full.px-3.py-1.text-sm.font-semibold.text-gray-700.mr-2.mb-2 uptime: #{p.uptime}
          span.inline-block.bg-gray-200.rounded-full.px-3.py-1.text-sm.font-semibold.text-gray-700.mr-2.mb-2 restarts: #{p.restarts}
